{
	"front":
	{
		"/":
		{
			"*":
			{
				"controller":"index",
				"action":"index"
			}
		},
		"deck/id:{$id_player}/":
		{
			"parameters":
			{
				"id_player":"[0-9]+"
			},
			"GET":
			{
				"controller":"player",
				"action":"display"
			}
		},
		"card/{$id_card}/":
		{
			"parameters":
            {
				"id_card": "[0-9]+"
			},
			"GET":
            {
				"controller": "search",
				"action": "card"
			}
		},
		"{$controller}/":
		{
			"parameters":
			{
				"controller":"[a-zA-Z0-9\\-]+"
			},
			"*":
			{
				"action":"index"
			}
		},
		"{$controller}/{$action}/":
		{
			"parameters":
			{
				"controller":"[a-zA-z0-9\\-]+",
				"action":"[a-zA-z0-9\\-]+"
			},
			"*":
			{
				"controller":"{$controller}",
				"action":"{$action}"
			}
		}
	},
	"back": {
		"/": {
			"*": {
				"controller": "index",
				"action": "index"
			}
		}
	}
}